{
    "name": "symplely/yaml",
    "description": "A Async simple YAML loader/dumper class for PHP",
    "type": "library",
    "keywords": [
        "async",
        "yaml",
        "yml"
    ],
    "homepage": "https://github.com/symplely/yaml/",
    "authors" : [
        {
        "name": "mustangostang",
        "email": "vlad.andersen@gmail.com"
        },
        {
        "name": "Lawrence Stubbs",
        "email": "lstubbs@techno.express"
    }],
    "license": "MIT",
    "require": {
        "php": "^7.0"
    },
    "autoload": {
        "files": [ 
            "Yaml/functions.php" 
        ],
        "psr-4": {
            "Async\\Yaml\\": "Yaml/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": ">5.7.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Async\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --bootstrap vendor/autoload.php tests"
    },
    "minimum-stability": "stable"
}
